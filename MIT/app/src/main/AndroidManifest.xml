<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mit_muzaffarpur">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Dashboard.Gallery.Gallery"></activity>
        <activity android:name=".Dashboard.Alumni.Alumni" />
        <activity android:name=".LoginSignup.RegisterActivity" />
        <activity android:name=".LoginSignup.LoginActivity" />
        <activity android:name=".LoginSignup.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Dashboard.AppInfo" />
        <activity android:name=".ClubProfile.ClubActivity" />
        <activity android:name=".Home.MainActivity" />
        <activity android:name=".Dashboard.DepartmentActivity" />
        <activity android:name=".Dashboard.Website" />
        <activity android:name=".Dashboard.About" />

        <service android:name=".Home.MyMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ic_stat_name" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />
    </application>

</manifest>